{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://educsbackendmysql.ytcb.org/api/$1"
    },
    {
      "source": "/models/(.*)",
      "destination": "https://educsbackendmysql.ytcb.org/api/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "https://educsbackendmysql.ytcb.org/uploads/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}